  <%= csrf_meta_tags %>
  <script src="https://checkout.stripe.com/checkout.js"></script>
</head>
<body>

  <table width="100%" border="0">
    <tr>
      <td colspan="3" style="background-color:#000000;color:#ff0099;text-align:center;height:110px;font-size: 33px;font-family: arial;">
        <h1>T R Y S T</h1>
      </td>
    </tr>

    <tr>
      <td style="background-color:#000000;width:500px;text-align:center;">
        <p><iframe src="//player.vimeo.com/video/87688245?title=0&amp;byline=0&amp;portrait=0&amp;" width="500" height="281" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe></p>
      </td>

      <td style="background-color:#FF0099;width:200px;font-family: arial;font-weight: bold;">
        <p>TECHNO</p>
        <p>VIDEO</p>
        <p>/-</p>
        <p><b>TRYST</b></p>
        <p>EXPERIENCE QUEER IN A/V</p>
        <p>/-</p>
        <p>04.05.14</p>
        <p>23:00-LATE</p>
        <p>LOCATION TBA</p>
        <p>/-</p>
        <p>LIMITED PRESALE $10</p>
        <p>FREE BEER BEFORE 1</p>
      </td>

      <td style="background-color:#00ccff;height:100%;width:200px;">

        <%= yield %>

      </td>
    </tr>

    <tr>
      <td colspan="3" style="background-color:#66ffcc;text-align:center;color:#000000;height:110px;font-family: arial;font-weight: bold;">
        QUESTIONS / COMMENTS / CONCERNS / NEED A COMP ? CONTACT EMAIL
      </td>
    </tr>
  </table>

</body>
</html>


